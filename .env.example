# Database Configuration
# Copy this file to .env and update with your actual credentials

DB_NAME=liquor_db
DB_USER=your_username
DB_PASSWORD=your_password_here
DB_HOST=localhost
DB_PORT=5432

# Git Configuration (for Airflow DAG commits)
GIT_AUTHOR_NAME=Your Name
GIT_AUTHOR_EMAIL=your.email@example.com
GIT_COMMITTER_NAME=Your Name
GIT_COMMITTER_EMAIL=your.email@example.com

# Project Paths
LIQUOR_APP_PATH=/path/to/liquor_app
CATALOG_BETA_PATH=/path/to/catalog-beta

# Airflow Configuration
AIRFLOW_DAG_OWNER=your_username
